@model MvcApplication1.Models.User.UserModel

    
<table>
    <tr><th>Pictures</th><td>@if(@Model.IsPhoto){<img src="@Model.Photo" />}</td></tr>
    <tr><th>User Name</th><td>@Model.UserName</td></tr>
    <tr><th>Name</th><td>@Model.Name</td></tr>
    <tr><th>Email</th><td>@Model.Email</td></tr>
    <tr><th>Comment</th><td>@Model.Comment</td></tr>
</table>


@if (Membership.GetUser() != null)
{
    if(Membership.GetUser().UserName.Equals(Model.UserName))
    {
        @Html.ActionLink("Edit", "edit", "user", new {id = Model.UserName}, new {@class = "btn large primary"})
    } 
}
